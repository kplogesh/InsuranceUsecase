pipeline {
    agent any 
    stages {
        stage('Build') { 
            steps {
                sh 'echo "Building.."'
                sh "chmod +x -R ${env.WORKSPACE}"
                sh '''
                    assets/insurance/pipeline/stages/build/build.sh
                '''
            }
        }
        stage('Test') { 
            steps {
                echo 'Testing'
            }
        }
        stage('Deploy') { 
            steps {
                echo 'Deploying'
            }
        }
    }
}